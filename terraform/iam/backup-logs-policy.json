{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::${backup_logs_s3_bucket}",
                "arn:aws:s3:::${backup_logs_s3_bucket}/*"
            ]
        }
    ]
}